{{#each this}}
    <li class="gallery-item">
        <div class="photo-card">
            <img src="{{webformatURL}}" alt="" width="400" height="250"/>
        
            <div class="stats">
                <p class="stats-item">
                    <i class="material-icons">thumb_up</i>
                    <span class="stats-elem">{{likes}}</span>
                </p>
                <p class="stats-item">
                    <i class="material-icons">visibility</i>
                    <span class="stats-elem">{{views}}</span>
                </p>
                <p class="stats-item">
                    <i class="material-icons">comment</i>
                    <span class="stats-elem">{{comments}}</span>
                </p>
                <p class="stats-item">
                    <i class="material-icons">cloud_download</i>
                    <span class="stats-elem">{{downloads}}</span>
                </p>
            </div>
        </div>
    </li>
{{/each}}

