{{#each this}}
<ul class="gallery">
    {{#each this}}
    <li class="gallery-item">
        <div class="photo-card">
            <img src="{{webformatURL}}" alt="" />
        
            <div class="stats">
                <p class="stats-item">
                    <i class="material-icons">{{likes}}</i>
                    1108
                </p>
                <p class="stats-item">
                    <i class="material-icons">{{views}}</i>
                    320321
                </p>
                <p class="stats-item">
                    <i class="material-icons">{{comments}}</i>
                    129
                </p>
                <p class="stats-item">
                    <i class="material-icons">{{downloads}}</i>
                    176019
                </p>
            </div>
        </div>
    </li>
    {{/each}}
</ul>
{{/each}}